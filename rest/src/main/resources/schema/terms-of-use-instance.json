{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "http://schema.envirocar.org/terms-of-use-instance.json#",
    "type": "object",
    "required": [
        "id",
        "issuedDate",
        "contents"
    ],
    "properties": {
        "created": {
            "type": "string",
            "format": "date-time"
        },
        "modified": {
            "type": "string",
            "format": "date-time"
        },
        "id": {
            "type": "string"
        },
        "issuedDate": {
            "type": "string",
            "format": "date"
        },
        "contents": {
            "type": "string"
        }
    }
}