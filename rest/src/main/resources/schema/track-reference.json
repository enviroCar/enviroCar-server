{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": [
    "id"
  ],
  "properties": {
    "id": {
      "type": "string"
    },
    "modified": {
      "type": "string",
      "format": "date-time"
    },
    "name": {
      "type": "string"
    },
    "begin": {
      "type": "string",
      "format": "date-time"
    },
    "end": {
      "type": "string",
      "format": "date-time"
    },
    "sensor": {
      "$ref": "sensor.json"
    },
    "length": {
      "type": "number"
    },
    "status": {
      "$ref": "track-status.json"
    }
  }
}